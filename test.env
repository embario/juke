### Runtime profile (required)
JUKE_RUNTIME_ENV=development
DJANGO_SECRET_KEY=<secret>

### Host ports (required)
BACKEND_PORT=
WEB_PORT=
RECOMMENDER_PORT=
REDIS_PORT=

### Internal ports (required)
POSTGRES_PORT=
EMAIL_PORT=

### Public URLs (required)
BACKEND_URL=
FRONTEND_URL=
CORS_ALLOWED_ORIGINS=

### Database (required)
POSTGRES_NAME=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

### Redis / Celery (required)
CELERY_BROKER_URL=
CELERY_RESULT_BACKEND=
# Seconds Redis keeps leased Celery tasks invisible before they can be requeued.
CELERY_VISIBILITY_TIMEOUT=3600

### Recommender engine (required)
RECOMMENDER_ENGINE_BASE_URL=
RECOMMENDER_ENGINE_TIMEOUT=15
RECOMMENDER_MODEL_VERSION=v1.0.0
RECOMMENDER_VECTOR_DIM=32

### Auth & email (required for auth flows)
EMAIL_HOST_USER=<host_user>
EMAIL_HOST_PASSWORD=<host_password>
DISABLE_REGISTRATION=0
